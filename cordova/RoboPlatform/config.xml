<?xml version='1.0' encoding='utf-8'?>
<widget id="io.web-manufacture.android.ilab.v3_8" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ILab 3.8 client</name>
    <description>
        ILab-3 client app to simple start with ilab
    </description>
    <author email="azzzooo@gmail.com" href="http://web-manufacture.net/technologies/Ilab">
        Web-Manufacture core team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-device" />
    <plugin name="cordovarduino" />
    <plugin name="cordova-plugin-bluetooth-serial" />
    <plugin name="org.vangulik.usb.hid" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/img/favicon-32x32.png" />
        <icon density="xhdpi" src="www/img/favicon-96x96.png" />
        <icon density="xxhdpi" src="www/img/favicon-144x144.png" />
        <icon density="xxxhdpi" src="www/img/favicon-192x192.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
