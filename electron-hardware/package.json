{
  "name": "Hlab-Electron",
  "version": "3.8.2",
  "electronVersion": "^9.1.0",
  "description": "HLab Electron Server app",
  "publishName": "HLab-Electron",
  "main": "electron-root.js",
  "author": "Semchenkov Alexander",
  "license": "CREATIVE COMMON",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebManufacture/ILAB-3.0.git"
  },
  "bugs": {
    "url": "https://github.com/WebManufacture/ILAB-3.0/issues"
  },
  "homepage": "https://github.com/WebManufacture/ILAB-3.0#readme",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "hlab-electron",
    "asar": false,
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      {
        "from": "../Modules",
        "to": "../Modules"
      },
      {
        "from": "../Services",
        "to": "../Services"
      },
      {
        "from": "../System",
        "to": "../System"
      },
      {
        "from": "../ILabClient",
        "to": "../ILabClient"
      },
      {
        "from": "..",
        "to": ".",
        "filter": [
          "*.js",
          "electron-config.json"
        ]
      },
      "../*.js",
      "../electron-config.json",
      "electron-config.json"
    ],
    "extraFiles": [],
    "mac": {
      "icon": "../favicon-310x310.png",
      "category": "application"
    },
    "linux": {
      "icon": "../favicon-310x310.png",
      "category": "application"
    },
    "win": {
      "icon": "../favicon-310x310.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "archiver": "^5.2.0",
    "chrome-remote-interface": "^0.31.0",
    "http-proxy": "latest",
    "jszip": "^3.5.0",
    "node-hid": "1.3.2",
    "serialport": "9.2.4",
    "usb-detection": "latest",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "devtron": "latest",
    "electron": "^10.4.0",
    "electron-builder": "22.5.0",
    "electron-devtools-installer": "^3.2.0"
  }
}
