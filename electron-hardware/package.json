{
  "name": "Ilab-Electron",
  "version": "3.8.2",
  "electronVersion": "^9.1.0",
  "description": "ILab Electron Server app",
  "publishName": "ILab-Electron",
  "main": "electron-root.js",
  "author": "Semchenkov Alexander",
  "license": "CREATIVE COMMON",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebManufacture/ILAB-3.0.git"
  },
  "bugs": {
    "url": "https://github.com/WebManufacture/ILAB-3.0/issues"
  },
  "homepage": "https://github.com/WebManufacture/ILAB-3.0#readme",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "ilab-electron",
    "mac": {
      "category": "your.app.category.type"
    },
    "extraFiles": [
      {
        "from": "Assets",
        "to": "resources"
      },
      {
        "from": "../Modules",
        "to": "Modules"
      },
      {
        "from": "../Services",
        "to": "Services"
      },
      {
        "from": "../System",
        "to": "System"
      },
      {
        "from": "../ILabClient",
        "to": "ILabClient"
      },
      {
        "from": "..",
        "to": ".",
        "filter": [
          "*.js",
          "electron-config.json"
        ]
      }
    ],
    "win": {
      "icon": "favicon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "http-proxy": "latest",
    "archiver": "^5.2.0",
    "serialport": "9.2.4",
    "usb-detection": "latest",
    "jszip": "^3.5.0",
    "node-hid": "1.3.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "electron-builder": "22.5.0",
    "electron": "^10.0.0"
  }
}
